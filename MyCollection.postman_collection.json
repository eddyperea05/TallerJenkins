	"item": [
		{
			"name": "Add a user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const jsonData = pm.response.json()\r",
							"\r",
							"pm.collectionVariables.set('userID',jsonData.data.id)\r",
							"\r",
							"pm.test(\"Status code should be 201\", ()=>{\r",
							"    pm.response.to.have.status(201)\r",
							"})\r",
							"\r",
							"\r",
							"/*pm.test(\"email should match\", ()=>{\r",
							"    pm.expect(jsonData.email).to.be.eq(\"edd@test.xco\")\r",
							"})*/"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"name\": \"{{name}}\",\r\n    \"email\": \"{{email}}\",\r\n    \"gender\": \"{{gender}}\",\r\n    \"status\": \"{{status}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{protocolo}}://{{url}}/{{visibility}}/{{version}}/users",
					"protocol": "{{protocolo}}",
					"host": [
						"{{url}}"
					],
					"path": [
